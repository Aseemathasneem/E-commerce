    <script>
        document.addEventListener("DOMContentLoaded", function () {
            // Get all quantity inputs, plus buttons, and minus buttons
            var quantityInputs = document.querySelectorAll(".cart__qty-input");
            var plusBtns = document.querySelectorAll(".qtyBtn.plus");
            var minusBtns = document.querySelectorAll(".qtyBtn.minus");
    
            // Attach event listeners to each row
            quantityInputs.forEach(function (quantityInput, index) {
                var plusBtn = plusBtns[index];
                var minusBtn = minusBtns[index];
    
                plusBtn.addEventListener("click", function () {
                    updateQuantity(quantityInput);
                });
    
                minusBtn.addEventListener("click", function () {
                    updateQuantity(quantityInput);
                });
    
                quantityInput.addEventListener("input", function () {
                    updateQuantity(quantityInput);
                });
            });
    
            function updateQuantity(inputElement) {
                var productId = getCurrentProductId(inputElement);
                var value = parseInt(inputElement.value) || 1;
    
    //             // Quantity check
    //             if (value === 1) {
    //                 Swal.fire({
    //     icon: 'warning',
    //     title: 'Quantity Warning',
    //     text: 'Quantity should be greater than 1',
    // });
    //                 return; // Stop further processing
    //             }
    
                // Make an AJAX request to update the quantity on the server
                fetch('/updateQuantity', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify({
                        productId: productId,
                        quantity: value,
                        // Add any other necessary data like user ID, etc.
                    }),
                })
                    .then(response => response.json())
                    .then(data => {
                        // Handle the response from the server if needed
                        console.log(data);
                        if (data.success) {
                            window.location.href = '/cart';
                        } else {
                            Swal.fire({
                                icon: 'error',
                                title: 'Not enough stock available',
                                text: 'There is not enough stock available for this product.',
                            });
                        }
                    })
                    .catch(error => {
                        console.error('Error:', error);
                    });
            }
    
            function getCurrentProductId(inputElement) {
                // Extract the product ID from the current row
                var currentRow = inputElement.closest('.cart__row');
                var productId = currentRow.getAttribute('data-product-id');
                console.log('Product ID:', productId);
                return productId;
            }
        });
    </script>
    

    
    
