        const success = urlParams.get('success');
        const message = urlParams.get('message');
        const discountAmount = parseFloat(urlParams.get('discountAmount')); // Convert to float
        const errors = urlParams.get('errors');

        // Function to create and append a message element to the page
        function appendMessage(type, content) {
            const messageElement = document.createElement('div');
            messageElement.classList.add(type);
            messageElement.textContent = content;
            document.getElementById('messages').appendChild(messageElement);
        }

        if (success === 'true') {
            // Display success message and discount amount
            appendMessage('success', `${message}. You saved ₹ ${discountAmount.toFixed(2)} !`);
            document.getElementById('discountAmount').textContent = `₹ ${discountAmount.toFixed(2)}`;

            const originalTotal = parseFloat(document.getElementById('dynamicGrandTotal').textContent);
            const newGrandTotal = originalTotal - discountAmount;

            if (!isNaN(newGrandTotal)) {
                document.getElementById('dynamicGrandTotal').textContent = newGrandTotal.toFixed(2);

                const checkoutButton = document.getElementById('cartCheckout');
                checkoutButton.href = `/checkout?newGrandTotal=${newGrandTotal.toFixed(2)}`;
            } else {
                // Handle the case where the result is NaN (e.g., invalid data)
                console.error('Error: Grand Total is not a number.');
            }
        } else {
            if (errors) {
                const errorMessages = JSON.parse(errors);
                if (errorMessages.length === 1) {
                    appendMessage('error', ` ${decodeURIComponent(errorMessages[0])}`);
                } else {
                    appendMessage('error', ` Errors: ${errorMessages.join(', ')}`);
                }
            } else {
                // If there are no errors provided, display a generic error message
                appendMessage('');
            }
        }

    </script>
    <script>
        document.getElementById('cartCheckout').addEventListener('click', function (event) {
            // Check if the checkbox is checked
            if (!document.getElementById('termsCheckbox').checked) {
                // If not checked, prevent the default action (navigating to /checkout)
                event.preventDefault();

                // Use SweetAlert to show a customized alert
                Swal.fire({
                    icon: 'error',
                    title: 'Oops...',
                    text: 'Please agree to the terms and conditions before proceeding to checkout.',
                });
            }
        });
    </script>

    
</body>

